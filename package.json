    "preview": "vite preview"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.11.0",
    "@types/node": "^25.0.3",
    "@types/react": "18.2.38",
    "@types/react-dom": "18.2.15",
    "eslint": "^9.39.2",
    "sass": "^1.95.1",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.2.5",
    "vite-plugin-dts": "^4.5.4"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "customElements": "custom-elements.json"

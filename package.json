{
  "name": "rezel",
  "description": "custom elements with responsive attributes",
  "version": "1.0.5",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/rezel.cjs.js",
  "module": "./dist/rezel.es.js",
  "types": "./dist/rezel.d.ts",
